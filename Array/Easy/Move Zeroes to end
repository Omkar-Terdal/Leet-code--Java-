class Solution {
    public void moveZeroes(int[] nums) {
        
        int n=nums.length;
        int i=0 ,j=0;
    //shift all non zero element to beginning
        while(j<n){
            if(nums[j]!=0){
            nums[i]=nums[j];
           
            i++;
            }
            j++;
        }
//remaining all element set to zero
          while(i<n){
                nums[i]=0;
                i++;
            }
    }
}
