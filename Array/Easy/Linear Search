import java.util.*;
public class Solution {
    public static int linearSearch(int n, int num, int []arr){
        // Write your code here.
        int result=-1;      //-1 for if element  not present in an arr
        for(int i=0; i<n; i++){
            if(arr[i]==num){
                result= i;
                break;
            }
        }
        return result;
    }
}
